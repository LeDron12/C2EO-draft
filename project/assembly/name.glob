c_int32 3 > g_a
c_int32 4 > g_b
c_int32 0 > g_c
[arg] > main
  seq > @
    ^.g_c.write (^.g_a.add (^.g_b))
    seq
      ^.g_c.write (^.g_a)
    ^.g_a.write (^.g_b)
    ^.g_b.write (^.g_c)
    seq
      stdout (^.g_a.as-string)
      stdout "\n"
    seq
      stdout (^.g_b.as-string)
      stdout "\n"
    seq
      stdout (^.g_c.as-string)
      stdout "\n"
main arg > @

