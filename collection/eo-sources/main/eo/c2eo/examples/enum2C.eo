+package c2eo.examples

+alias c2eo.stdio.printf
+alias c2eo.ctypes.c_int32

[args] > enum2C

  6 > currentDay
  c_int32 > someday
  c_int32 > anotherDay

  currentDay > getSomeDay

  seq > main
    someday.write 2
    printf "enum2 someday = %d\n" someday
    anotherDay.write getSomeDay
    printf "enum2 another day = %d\n" anotherDay
