+package c2eo
+alias varint c2eo.varInt

# neg varInt -> varInt
[x r] > negInt
  r.set ((x.get).neg) > @

# add varInt varInt -> varInt
[x y r] > addInt
  r.set ((x.get).add (y.get)) > @

# sub varInt varInt -> varInt
[x y r] > subInt
  r.set ((x.get).sub (y.get)) > @

# mul varInt varInt -> varInt
[x y r] > mulInt
  r.set ((x.get).mul (y.get)) > @

# div varInt varInt -> varInt
[x y r] > divInt
  r.set ((x.get).div (y.get)) > @

# mod varInt varInt -> varInt
[x y r] > modInt
  r.set ((x.get).mod (y.get)) > @

# pow varInt varInt -> varInt
[x y r] > powInt
  r.set ((x.get).pow (y.get)) > @

