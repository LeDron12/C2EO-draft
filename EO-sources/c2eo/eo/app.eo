    stdout
      sprintf
        "subInt i1,i2->i3: %s\n"
        i3.toString

    mulint i1 i2 i3
    stdout
      sprintf
        "mulInt i1,i2->i3: %s\n"
        i3.toString

    divint i1 i2 i3
    stdout
      sprintf
        "divInt i1,i2->i3: %s\n"
        i3.toString

    modint i1 i2 i3
    stdout
      sprintf
        "modInt i1,i2->i3: %s\n"
        i3.toString

    powint i1 i2 i3
    stdout
      sprintf
        "powInt i1,i2->i3: %s\n"
