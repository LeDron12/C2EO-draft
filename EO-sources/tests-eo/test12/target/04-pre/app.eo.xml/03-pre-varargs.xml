   <listing>+package test12
+alias stdout org.eolang.io.stdout
+alias sprintf org.eolang.txt.sprintf

[] &gt; var
  memory &gt; value!
  [y] &gt; set
    ^.value.write y &gt; @
  [] &gt; get
    ^.value &gt; @

[] &gt; varInt
  "int" &gt; type
  var &gt; @

  [] &gt; toString
    sprintf &gt; @
      "%d"
      ^.get

# Попытка выполнить цикл
[] &gt; app
  varInt &gt; i!
  seq &gt; @
    i.set 1
    while.
      less.
        i.get
        10
      seq
        stdout
          sprintf
            "i = %d"
            i.get
        i.write
          add.
            1
            i.get
&lt;EOF&gt;</listing>
